question===>https://www.codechef.com/problems/TRICOIN


#include<bits/stdc++.h>
#define int long long 
using namespace std;
signed main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int low = 1,high=n;
        while(low<=high){
            int mid = (low+high)/2;
            if(((mid*(mid+1))/2)>n){
                high = mid-1;
            }
            else{
                low = mid+1;
            }
        }
        cout<<high<<endl;
    }
    return 0;
}
